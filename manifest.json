{
  "name": "Codeforces.org redirector",
  "description": "Redirects requests from codeforces.org to codeforces.com/codeforces.org, making Codeforces work again",
  "version": "0.1",
  "manifest_version": 2,
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "*://userpic.codeforces.org/*",
    "*://codeforces.com/*",
    "*://codeforces.org/*"
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "blendervik@gmail.com",
      "strict_min_version": "58.0"
    }
  },
  "background": {
    "scripts": ["background.js"]
  },
  "icons": {
    "48": "icons/icon48.png"
  }
}
